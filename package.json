    "@redux-devtools/core": "^3.13.1",
    "@types/node-sass": "^4.11.3",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "airbnb": "^0.0.2",
